		LocDirectEntry
		{
			Key = "$randomEvents.neutral.narration.1";
			Text = "Sei un visionario!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.narration.2";
			Text = "Va tutto bene";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.narration.3";
			Text = "Così è la vita";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.narration.4";
			Text = "È una vera sfortuna.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.narration.5";
			Text = "Sarà sicuramente utile";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.1";
			Text = "Sei il primo a ricevere notizie sull'ultima moda a Parigi, e così decidi di attaccare diversi nastri colorati al tuo cappello.\n\nQuesto porta i cittadini di mentalità ristretta a sorriderti stranamente...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.2";
			Text = "In un sogno, voli come un uccello attraversando tutti i paesi e i mari del nostro mondo per arrivare finalmente a casa, volando sempre per linea diritta.\n\nSenza ulteriori indugi, prendi la mappa del mondo dal muro del tuo laboratorio e la incolli su una grande palla di legno. I tuoi soci in affari sono >piuttosto< irritati, ma ti lasciano fare la tua piccola stravaganza...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.3";
			Text = "Un carro trainato da cavalli corre impetuosamente per la strada, attraverso una strada fangosa. Se non ti fossi fatto da parte in fretta, i tuoi vestiti raffinati sarebbero stati rovinati dal fango maleodorante...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.4";
			Text = "Tuo padre ti ha sempre detto di essere sempre diligente e onesto. Dovresti ascoltare il suo saggio consiglio!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.5";
			Text = "Teoricamente avresti un sacco di importanti affari da sbrigare, ma semplicemente non riesci a concentrarti...hai chiuso bene la porta di casa tua prima di uscire? Ci sono così tanti truffatori per strada...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.6";
			Text = "Spensierato, cammini lungo un sentiero quando all'improvviso qualcuno ti prende la cintura e con un taglio velocissimo del suo coltello taglia la cinghia della tua borsa. Prima di poter chiamare le guardie è gia scomparso nella stradina laterale. Poco dopo senti un >bong< smorzato provenire dalla stessa stradina. Dai un'occhiata e non puoi fare a meno di tirare un sospiro di sollievo.: l'impudente ladro è sbattuto con la testa contro la corazza di una guardia della città quando è corso dietro l'angolo. Ora il ladro giace privo di sensi a terra...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.7";
			Text = "Che sogno meraviglioso hai avuto la scorsa notte! \nNo, era più una visione! \nHai sognato un pasto così delizioso, che solo il tuo palato poteva davvero apprezzarlo: d'ora in avanti >Formaggio su pane tostato< sarà il tuo piatto preferito...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.8";
			Text = "All'improvviso, senti un enorme voglia di mangiare una patata. Ma dove puoi trovarne? Devi sopprimere il tuo desiderio, ma non riesci a farlo...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.9";
			Text = "Sei stanco di essere sempre costretto a mangiare solo con la mano destra. Perché qualcuno non inventa un panno o una carta con cui puoi pulirti il sedere dopo essere andato in bagno?";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.10";
			Text = "Un gatto nero sta attraversando la tua strada da destra a sinistra ... Ora chiediti: sei superstizioso?";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.11";
			Text = "Questa è l'ultima volta che bevi birra Guntram ... Se non avessi passato così tanto tempo a guardare attraverso il fondo del bicchiere la notte scorsa, non avresti avuto un mal di testa oggi. Impara dai tuoi errori!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.12";
			Text = "La scorsa notte hai visto una stella cadente. Era un angelo che cadeva dal cielo in terra?";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.13";
			Text = "i sei sempre chiesto quanto sei probabile che tu sia colpito da un fulmine. La risposta è è probabilmente difficile quanto capire un giorno la creazione divina! Tuttavia, poiché Dio usa un fulmine come punizione per i peccatori, ci può essere un modo in cui puoi influenzare quella probabilità..";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.14";
			Text = "Di recente hai sentito che hanno escogitato nuove regole per gli scacchi; questi includono l'arrocco. Sicuramente sarai in grado di usarlo a tuo vantaggio nella tua prossima partita.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.15";
			Text = "Ti fermi ancora una volta per riflettere e infine giungere alla conclusione che anche se una borsa piena di soldi da sola non può renderti felice, sicuramente può fornire una certa rassicurazione...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.16";
			Text = "Ancora una volta, la tua borsa è gonfia di così tante monete che la cintura non riesce a tenere i pantaloni. Probabilmente delle lettere di debito scritte su carta o pergamene sarebbero più convenienti, ma probabilmente nessuno le accetterebbe...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.17";
			Text = "Durante una passeggiata in città, vedi un uomo fischiare felicemente mentre bussa ad una porta ed esclama: >Apri, mia cara moglie, sono tornato<.\nQuando l'uomo entra in casa, al primo piano si apre una finestra e un uomo mezzo nudo, con un fascio di vestiti sotto il braccio, scende in fretta...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.18";
			Text = ">Ehi, ho qualcosa per te<.\n\nCon un sorriso che potrebbe sciogliere il ghiaccio, una bambina ti dà una margherita. \n\n>Questo dovrebbe portarti fortuna. Penso che ne hai bisogno più di me!<";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.19";
			Text = "Un cane che dorme sul ciglio della strada improvvisamente si alza, e per caso, sfiora i tuoi pantaloni. Pensi di aver visto diversi punti neri saltare dal suo pelo sul tuo pantalone...\n\nCorri al più vicino allevamento di maiali, ti siedi e velocemente inizi a coprire il tuo pantalone di fango.\nCon questo dovresti sbarazzarti anche dell'ultima pulce!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.20";
			Text = "Un sacco di grano cade a Costantinopoli!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.21";
			Text = "Camminando nel mercato trovi un documento ingiallito. Con un po' di sforzo riesci a decifrare le parole >questo è Il canto dei Nibelunghi<. Fai spallucce e lo rimetti dove lo hai trovato.\n\nPoco dopo, un uomo chiaramente istruito raccoglie il documento e grida felicemente: >Che incredibile scoperta! Mi farà guadagnare una fortuna!<";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.neutral.22";
			Text = "Ricevi una benedizione sotto mentite spoglie. Se avessi calpestato dello sterco di vacca e ti fossi fermato perchè infastidito, saresti stato investito da un carro trainato da cavalli solo un secondo dopo...";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.narration.1";
			Text = "La fortuna è dalla tua parte!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.narration.2";
			Text = "Piaci a qualcuno!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.narration.3";
			Text = "Qualcosa di incredibile!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.narration.4";
			Text = "Sei rispettato!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.narration.5";
			Text = "Lo aspettavi da molto tempo!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.1";
			Text = "Grazie ad una fortunata coincidenza, sei stato in grado di prevenire un attacco a un edificio appartenente a %fullname%.\n\n Puoi essere certo della gratitudine della famiglia %dynasty%!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.2";
			Text = "Al mercato noti che i pantaloni di %fullname% non sono abbottonati in modo appropriato come dovrebbero. Grazie alla tua osservazione discreta, la famiglia %dynasty% si è salvata da una situazione imbarazzante, che aumenta la tua reputazione nei loro confronti.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.3";
			Text = "Per caso, noti un borseggiatore rubare il portafogli di %fullname%. Senza un attimo di esitazione, insegui il ladro.\n L'intera famiglia %dynasty% ti esprime la sua gratitudine.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.4";
			Text = "Qualcuno ha ingaggiato un assassino per uccidere %fullname%! Tuttavia, eri nel posto giusto al momento giusto e sei stato in grado di spaventare l'assassino gridando > assassino! Aiuto!<. \n La famiglia %dynasty% ti è molto grata";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.5";
			Text = "In un dibattito pubblico sui diritti commerciali, ti schieri dal lato di %fullname%.\nAnche se non avevi intenzione di farlo, la famiglia %dynasty% ora ti vede con una luce più amichevole..";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.6";
			Text = "Vinci %coins% mentre giochi a carte dopo la messa!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.7";
			Text = "La morte di un parente lontano ti porta un'eredità di %coins%!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.8";
			Text = "In un campo appena fuori dalla città trovi alcuni frammenti antichi. \n\nNonostante tu insista che non sono altro che ciondoli, ti viene consegnata una borsa nella piazza del mercato contenente una ricompensa di %coins% per la tua scoperta.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.9";
			Text = "Il consiglio comunale ha deciso di rimborsarti %coins% sulle tue tasse pagate in eccesso. \nFinalmente!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.10";
			Text = "Guadagni %coins% dalla speculazione immobiliare.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.11";
			Text = "Eri sulla buona strada per un incontro importante con un socio nel quartiere >%neighborhood%< della città di %city% quando un cavallo da tiro si è impaurito e ha iniziato a galoppare dritto verso una bambina. Ti sei subito precipitato in soccorso e salvato la bambina.\n\nGli abitanti del quartiere sono entusiasti del tuo eroico coraggio!";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.12";
			Text = "Passeggi allegramente attraverso il quartiere di %neighborhood% nella città di %city% e saluti tutti quelli che incontri con un parole amichevoli. Anche se le persone sono effettivamente sorprese, ti sorridono.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.13";
			Text = "Le tue attività nel quartiere di %neighborhood% nella città di %city% hanno portato ricchezza a molti dei cittadini che vivono lì. Siete accolto in modo molto più amichevole di prima ogni volta che vi trovate nel quartiere.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.14";
			Text = "Per caso, hai molti avanzi della festa di ieri nella tua residenza, che potresti gettare ai maiali. Invece, li dai ai poveri del quartiere %neighborhood% nella città di %city% e ti assicuri che il nome del nobile donatore sia reso noto a tutti.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.15";
			Text = "Hai dato ad una povera ragazza nel quartiere %neighborhood% della città di %city% un pezzo di pane secco. E il giorno dopo l'intero quartiere sapeva della tua carità. Forse dovresti distribuire il pane ai poveri più spesso..";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.16";
			Text = "Il rapporto di viaggio di un alto funzionario statale che ha appena visitato la città menziona esplicitamente la tua famiglia. La tua reputazione nell'impero aumenta di conseguenza.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.17";
			Text = "Uno dei tuoi fidati informatori riferisce che le forze nemiche stanno pianificando di avvelenare il tuo sovrano... Invii immediatamente un dispaccio al tribunale di %kingdom%. Puoi star certo che il tuo nome ora riceverà sicuramente più attenzione.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.18";
			Text = "Trovi 1 %itemname% sulla strada, con evidenti segni di usura, ma per il resto in ottime condizioni.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.19";
			Text = "Semplicemente non puoi rifiutare un buon affare: acquisti 1 %itemname% per la modica somma di %coins% Taler, da un mercante con problemi mentali.";
		},
		LocDirectEntry
		{
			Key = "$randomEvents.positive.20";
			Text = "Per festeggiare il compleanno di un parente lontano, porti alcune bottiglie di vino ad un incontro al municipio. Anche se, a posteriori, non sono state prese decisioni significative, è stata una giornata davvero divertente. \n\nI membri del consiglio cittadino della citta di %city%, ti sono in ogni caso grati per questo piccolo diversivo...";
		},
		LocDirectEntry
